<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Plone.de Theme">

 <object name="plonede_images"
    meta_type="Filesystem Directory View"
    directory="plonede.theme:skins/plonede_images"/>
 <object name="plonede_js"
    meta_type="Filesystem Directory View"
    directory="plonede.theme:skins/plonede_js"/>
 <object name="plonede_templates"
    meta_type="Filesystem Directory View"
    directory="plonede.theme:skins/plonede_templates"/>
 <object name="plonede_styles"
    meta_type="Filesystem Directory View"
    directory="plonede.theme:skins/plonede_styles"/>

 <skin-path name="Plone.de Theme" based-on="Sunburst Theme">
  <layer name="plonede_images"
     insert-after="custom"/>
  <layer name="plonede_js"
     insert-after="plonede_images"/>
  <layer name="plonede_templates"
     insert-after="plonede_js"/>
  <layer name="plonede_styles"
     insert-after="plonede_templates"/>
 </skin-path>

</object>
